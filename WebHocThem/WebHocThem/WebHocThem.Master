                });

                $(".button_lotrinh div").click(function(){
                    $(".button_lotrinh div").removeClass('active');
                    $("#lotrinh2").hide(); $("#lotrinh4").hide();
                    $(this).addClass('active');
                    var id = $(this).data('id');
                    $("#lotrinh"+id).show();
                });

                $(".thumbnail-video").click(function() {
                    var str = $(this).find('img').data('video');
                    if (str == "") {
                        return false;
                    }
                    var res = str.split("=");
                    var embeddedUrl = "https://www.youtube.com/embed/" + res[1] + "?autoplay=1";
                    $(this).html('<iframe src="' + embeddedUrl + '" allowfullscreen="" frameborder="0" width="100%" height="100%" style="min-height: 350px" allow="autoplay"></iframe>'
                    );
                });

                if (! localStorage.getItem("showCourseFree")) {
                    var duration = $('#courseFree').attr('data-duration');
                    setTimeout(function () {
                        $('#courseFree').modal('show');
                        localStorage.setItem("showCourseFree", '1');
                    }, parseInt(duration)*1000);
                }
            })
